{
  "name": "ionic-docs",
  "displayName": "Ionic Docs",
  "description": "Documentation for ionic",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "main": "./out/extension.js",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "node-fetch": "^2.6.0"
  },
  "activationEvents": [
    "onCommand:extension.openDoc",
    "onCommand:extension.openDocExternal"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.openDoc",
        "title": "Ionic Documentation"
      },
      {
        "command": "extension.openDocExternal",
        "title": "Ionic Documentation: Website"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.openDoc"
        },
        {
          "command": "extension.openDocExternal"
        }
      ]
    },
    "configuration": [
      {
        "title": "Ionic Documentation Configuration",
        "properties": {
          "ionic.documentation.version": {
            "type": "number",
            "enum": [
              3,
              4
            ],
            "default": 4,
            "description": "Select the version for the ionic documentation",
            "scope": "window"
          }
        }
      }
    ]
  }
}